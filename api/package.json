{
  "name": "portchain-challenge",
  "version": "1.0.0",
  "description": "Portchain coding challange",
  "scripts": {
    "develop": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/local.ts",
    "watch-node": "nodemon ./dist/local.js",
    "watch-ts": "tsc -w",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test:coverage": "nyc --reporter=html mocha --require ts-node/register test/**/*.ts",
    "lint": "eslint --ext=ts --fix ./src",
    "deploy:demo": "serverless deploy --stage demo"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.152",
    "@types/mocha": "^7.0.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.4",
    "@types/sinon": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "aws-sdk": "^2.682.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "serverless-webpack": "^5.3.2",
    "sinon": "^9.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "serverless-http": "^2.5.0",
    "source-map-support": "^0.5.10"
  },
  "prettier": {},
  "author": "Mustafa Ekrem KENTER",
  "license": "MIT",
  "private": true
}
